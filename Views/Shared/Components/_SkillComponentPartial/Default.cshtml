@model List<MyPortfolio.DataAccessLayer.Entities.Skill>
<div class="col-six tab-full right">
    <h3>Programlama Dilleri & Teknolojiler</h3>

    <ul class="skill-bars">
        @foreach(var item in Model){
            string progress = "progress percent"+item.Value;
        <li>
            <div class="@progress"><span>@item.Value%</span></div>
            <strong>@item.Title</strong>
        </li>
        }
    </ul>
</div>